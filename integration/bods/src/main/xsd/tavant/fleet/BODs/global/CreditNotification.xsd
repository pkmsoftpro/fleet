<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://www.tavant.com/fleet/creditnotification">	<xs:include schemaLocation="../../Resources/Nouns/global/ApplicationAreaType.xsd" />	<xs:element name="CreditNotification">		<xs:complexType>			<xs:sequence>				<xs:element name="ApplicationArea" type="ApplicationAreaType"/>									<xs:element name="DataArea">					<xs:complexType>						<xs:sequence>							<xs:element name="CreditNotificationInfo">								<xs:complexType>									<xs:sequence>										<xs:element name="SalesOrganization">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="4" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DistributionChannel">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="2" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="Division">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="2" />												</xs:restriction>											</xs:simpleType>										</xs:element>																			<xs:element name="ClaimNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="20" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="LocationNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="12" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="ContractCode">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="12" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CustomerNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="12" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DealerNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="12" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="Currency">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="3" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="ResponseType">											<xs:annotation>                   								 <xs:documentation>Destination system for current integration. eg: SLMS</xs:documentation>                							</xs:annotation>											 <xs:simpleType>									            <xs:restriction base="xs:string">									                <xs:maxLength value="1"/>									                <xs:enumeration value="I"/>									                <xs:enumeration value="R"/>									                <xs:enumeration value="S"/>									            </xs:restriction>									      	  </xs:simpleType>										</xs:element>									</xs:sequence>								</xs:complexType>															</xs:element>							<xs:element name="InitialResponseInfo" minOccurs="1" maxOccurs="1">								<xs:complexType>									<xs:sequence>										<xs:element type="xs:date" name="DMRDocDate" />										<xs:element name="CustomerDMRDocId">											<xs:annotation>												<xs:documentation>Debit Memo Request Number for Customer</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="10" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CustomerDMRTotalAmount">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CustomerDMRTaxAmount">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>							<xs:element name="InvoicingResponseInfo" minOccurs="0">								<xs:complexType>									<xs:sequence>										<xs:element type="xs:date" name="CMRDocDate" />										<xs:element name="DealerCMRDocId">											<xs:annotation>												<xs:documentation>Credit Memo Request Number for Dealer</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="10" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DealerTotalAmount">											<xs:annotation>												<xs:documentation>Total amount of the Dealer Claim which will appear on the Dealer Credit Memo Request</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DealerTaxAmount">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>											<xs:element type="xs:date" name="CustomerInvDocDate" />										<xs:element name="CustomerInvDocId">											<xs:annotation>												<xs:documentation>Debit Memo Request Number for Customer</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="10" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CustomerInvTotalAmount">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CustomerInvTaxAmount">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>									</xs:sequence>								</xs:complexType>							</xs:element>														<xs:element name="SupplementalResponseInfo" minOccurs="0">							<xs:complexType>									<xs:sequence>										<xs:element name="SuppBillingParty">										<xs:annotation>												<xs:documentation>transaction for C - Customer, D - Dealer</xs:documentation>											</xs:annotation>											 <xs:simpleType>									            <xs:restriction base="xs:string">									                <xs:maxLength value="1"/>									                <xs:enumeration value="C"/>									                <xs:enumeration value="D"/>									            </xs:restriction>									      	  </xs:simpleType>										</xs:element>										<xs:element name="SuppBillingType">										<xs:annotation>												<xs:documentation>transaction for C - Credit, D - Debit</xs:documentation>											</xs:annotation>											 <xs:simpleType>									            <xs:restriction base="xs:string">									                <xs:maxLength value="1"/>									                <xs:enumeration value="C"/>									                <xs:enumeration value="D"/>									            </xs:restriction>									      	  </xs:simpleType>										</xs:element>										<xs:element name="SuppBillingDoc" minOccurs="0">											<xs:annotation>												<xs:documentation>Supplementary transaction document number</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="15" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element type="xs:date" name="SuppBillingDocDate" />										<xs:element name="SuppBillReqDoc" minOccurs="0">											<xs:annotation>												<xs:documentation>Supplementary transaction request document </xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="15" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element type="xs:date" name="SuppBillReqDocDate" />										<xs:element name="SuppTotalAmount">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="SuppTaxAmount">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="999999999999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										</xs:sequence>							</xs:complexType>							</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>