<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.2//EN"   
    "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>

	 <field name="limitOfAuthorityScheme.code">
		<field-validator type="requiredstring">
			<param name="trim">true</param>		
			<message key="error.manageLAOScheme.codeRequired"/>
		</field-validator>
	</field>
	
	 <field name="limitOfAuthorityScheme.name">
		<field-validator type="requiredstring">
			<param name="trim">true</param>		
			<message key="error.manageLAOScheme.nameRequired"/>
		</field-validator>
	</field>
	
	 <field name="limitOfAuthorityScheme.description">
		<field-validator type="requiredstring">
			<param name="trim">true</param>		
			<message key="error.manageLAOScheme.descRequired"/>
		</field-validator>
	</field>
	
	<field name="limitOfAuthorityScheme.loaLevels">
        <field-validator type="visitor">
            <param name="appendPrefix">true</param>
            <message/>
        </field-validator>
    </field>
  		
</validators>