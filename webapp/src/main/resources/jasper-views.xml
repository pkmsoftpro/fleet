<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

    <bean name="quotePdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/quote.jasper"/>
    </bean>
    
    <bean name="fleetClaimPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/fleetClaim.jasper"/>
    </bean>
    <bean name="invoiceTANMPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/invoice_TimeAndMaterialPdf.jasper"/>
    </bean>
    <bean name="invoiceFixAndHourlyPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/invoice_FixAndHourlyPdf.jasper"/>
    </bean>
    
    <bean name="customerProgramGuideSummaryPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/customerProgramGuideSummary.jasper"/>
    </bean>
    
    <bean name="dealerProgramGuideSummaryPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/dealerProgramGuideSummary.jasper"/>
    </bean>
    
    <bean name="programGuideSummaryPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/programGuideSummary.jasper"/>
    </bean>
    
    <bean name="contractPdf" class="org.springframework.web.servlet.view.jasperreports.JasperReportsPdfView">
        <property name="url" value="classpath:/reports/contract.jasper"/>
    </bean>
    
</beans>
