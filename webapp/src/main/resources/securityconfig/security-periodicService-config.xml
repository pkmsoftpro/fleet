<?xml version="1.0" encoding="UTF-8"?>
<functionalareas xmlns="http://www.tavant.com/twms/security/resource"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.tavant.com/twms/security/resource
			http://www.tavant.com/twms/security/resource">
	<functionalarea name="periodicService" defaultpolicy="none">

	<resource type="ui" value="periodicServiceTab" operation="OR">
		<permissions>periodicServices:periodicServiceTab:update,periodicServices:periodicServiceTab:view
		</permissions>
	</resource>

	<resource type="ui" value="serviceSchedules" operation="OR">
		<permissions>periodicServices:serviceSchedules:update,periodicServices:serviceSchedules:create
		</permissions>
	</resource>

	<resource type="ui" value="searchPeriodicServices" operation="OR">
		<permissions>periodicServices:searchPeriodicServices:update,periodicServices:searchPeriodicServices:search
		</permissions>
	</resource>      
        
        
	</functionalarea>

</functionalareas>
