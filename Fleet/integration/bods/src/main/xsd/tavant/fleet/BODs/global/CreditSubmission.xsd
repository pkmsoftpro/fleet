<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="unqualified" targetNamespace="http://www.tavant.com/fleet/creditsubmission"><xs:include schemaLocation="../../Resources/Nouns/global/ApplicationAreaType.xsd" />	<xs:element name="CreditSubmission">		<xs:complexType>			<xs:sequence>				<xs:element name="ApplicationArea" type="ApplicationAreaType"/>				<xs:element name="DataArea">					<xs:complexType>						<xs:sequence>							<xs:element name="ClaimInfo">								<xs:complexType>									<xs:sequence>										<xs:element name="SalesOrganization">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="4" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DistributionChannel">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="2" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="Division">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="2" />												</xs:restriction>											</xs:simpleType>										</xs:element>																			<xs:element name="CustomerNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="10" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="LocationNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="10" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DealerCode">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="12" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DealerTechId" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="10" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="Currency">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="5" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="EquipNbr">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="18" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="SerialNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="18" />												</xs:restriction>											</xs:simpleType>										</xs:element>																				<xs:element name="MaterialCode">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="18" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CallType">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="4" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="ComponentCode">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="3" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="ServiceCallId">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="17" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="WorkOrderNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="17" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="PONumber" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="20" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="ClaimNumber">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="12" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DealerClaimNumber" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="10" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="ClaimYear" minOccurs="0"> 											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="4" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="StartRepairDate">											<xs:annotation>                   								<xs:documentation>mm-dd-yyyy </xs:documentation>               								</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:date"/>											</xs:simpleType>										</xs:element>											<xs:element name="EndRepairDate">											<xs:annotation>                   								<xs:documentation>mm-dd-yyyy </xs:documentation>               								</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:date"/>											</xs:simpleType>										</xs:element>										<xs:element name="Notes">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="4000" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<!-- total labor amount-->										<xs:element name="LaborAmount" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999999999999999" />													<xs:fractionDigits value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<!-- SUM of parts amount-->										<xs:element name="PartsAmount" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999999999999999" />													<xs:fractionDigits value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<!-- SUM of MISC amount-->										<xs:element name="MiscAmount" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999999999999999" />													<xs:fractionDigits value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<!-- SUM of Travel amount-->										<xs:element name="TravelAmount" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999999999999999" />													<xs:fractionDigits value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<!-- Total Claim Amount, equals sum of Labor + Parts + Misc +Drayage + Travel-->										<xs:element name="TotalClaimAmount" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999999999999999" />													<xs:fractionDigits value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>																			<xs:element name="MeterReading" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:integer"/>											</xs:simpleType>										</xs:element>											<xs:element name="TravelHours" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="LaborHours" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999" />													<xs:fractionDigits value="2"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="LaborRate" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999999999999999" />													<xs:fractionDigits value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="DMRDetailLine" minOccurs="0" maxOccurs="unbounded">											<xs:complexType>												<xs:sequence>													<xs:element name="LineNumber" minOccurs="0">											<xs:annotation>												<xs:documentation>Sequential number representing the labor amount, travel amount, etc</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:integer"/>											</xs:simpleType>										</xs:element>										<xs:element name="RecordType">											<xs:annotation>												<xs:documentation>Either PARTS OR MISC </xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:enumeration value="PARTS" />													<xs:enumeration value="MISC" />												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="SubCostCode">											<xs:annotation>												<xs:documentation>Sub Cost Code Part number enter in oem/non-oem</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="15"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="ItemDescription">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="50"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="Quantity" minOccurs="0">											<xs:annotation>												<xs:documentation>Quantity</xs:documentation>											</xs:annotation>											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:maxLength value="12"/>												</xs:restriction>											</xs:simpleType>										</xs:element>										<xs:element name="CostAmount" minOccurs="0">											<xs:simpleType>												<xs:restriction base="xs:decimal">													<xs:maxInclusive value="9999999999999999999" />													<xs:fractionDigits value="5"/>												</xs:restriction>											</xs:simpleType>										</xs:element>													</xs:sequence>											</xs:complexType>										</xs:element>									</xs:sequence>								</xs:complexType>															</xs:element>						</xs:sequence>					</xs:complexType>				</xs:element>			</xs:sequence>		</xs:complexType>	</xs:element></xs:schema>